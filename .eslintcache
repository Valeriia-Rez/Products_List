const CartViewPage = () => {\n  const [state, dispatch] = useStore();\n\n  const fetchData = async () => {\n    const products = await axios(\"http://localhost:8000/cart\");\n    dispatch(\"ADD_TO_CART\", products.data);\n  };\n\n  useEffect(() => {\n    if (!state.cart.length) {\n      fetchData();\n    }\n  }, [state.cart.length]);\n\n  const onDeleteHandler = async (id: string | number) => {\n    await axios.patch(`http://localhost:8000/products/${id}`, {\n      inCart: false,\n    });\n    const updatedProducts = state.products.map((product: IProduct) => {\n      if (product.id === id) {\n        product.inCart = false;\n      }\n      return product;\n    });\n    dispatch(\"SET_PRODUCTS\", updatedProducts);\n    await axios.delete(`http://localhost:8000/cart/${id}`);\n    dispatch(\"REMOVE_FROM_CART\", id);\n  };\n\n  const onDecreaseQuantity = async (id: string | number, quantity: number) => {\n    if (quantity - 1 >= 1) {\n      await axios.patch(`http://localhost:8000/cart/${id}`, {\n        quantity: quantity - 1,\n      });\n      dispatch(\"DEACREASE_QUANTITY\", id);\n    }\n  };\n\n  const onIncreaseQuantity = async (id: number | string, quantity: number) => {\n    await axios.patch(`http://localhost:8000/cart/${id}`, {\n      quantity: quantity + 1,\n    });\n    dispatch(\"INCREASE_QUANTITY\", id);\n  };\n\n  return (\n    <div className=\"cart\">\n      <div className=\"cart__wrapper\">\n        {state.cart.length ? (\n          state.cart.map((product: ICartItem) => {\n            return (\n              <div key={product.id} className=\"cart__product\">\n                <h1>{product.title}</h1>\n                <span>Price: {product.price}</span>\n                <p>Description: {product.description}</p>\n                <p>\n                  quantity:\n                  <Button\n                    buttonName=\"-\"\n                    onClick={() =>\n                      onDecreaseQuantity(product.id, product.quantity)\n                    }\n                    className=\"button-sm\"\n                  />\n                  {product.quantity}\n                  <Button\n                    buttonName=\"+\"\n                    onClick={() =>\n                      onIncreaseQuantity(product.id, product.quantity)\n                    }\n                    className=\"button-sm\"\n                  />\n                </p>\n                <Button\n                  onClick={() => onDeleteHandler(product.id)}\n                  buttonName=\"Delete\"\n                  className=\"button\"\n                />\n              </div>\n            );\n          })\n        ) : (\n          <p className=\"cart__message\">Your cart is empty. </p>\n        )}\n      </div>\n      {state.cart.length >= 1 && (\n        <div className=\"cart__info\">\n          <div>\n            Products quantity: {totalQuantity(state.cart)}\n            <span className=\"cart__infoItem\"></span>\n          </div>\n          <div>\n            Total amount: {state.cart.length && totalAmount(state.cart)}\n            <span className=\"cart__infoItem\"></span>\n          </div>\n        </div>\n      )}\n    </div>\n  );\n};\n\nexport default CartViewPage;\n","/mnt/c/Users/vihro/lera/products_list/src/pages/MainViewPage/MainViewPage.tsx",["103"],"/mnt/c/Users/vihro/lera/products_list/src/components/Layout/Layout.tsx",[],"/mnt/c/Users/vihro/lera/products_list/src/pages/CreateViewPage/CreateViewPage.tsx",[],"/mnt/c/Users/vihro/lera/products_list/src/pages/EditViewPage/EditViewPage.tsx",[],"/mnt/c/Users/vihro/lera/products_list/src/components/Button/index.ts",[],"/mnt/c/Users/vihro/lera/products_list/src/components/Card/index.ts",[],"/mnt/c/Users/vihro/lera/products_list/src/components/Footer/index.ts",[],"/mnt/c/Users/vihro/lera/products_list/src/components/Header/index.ts",[],"/mnt/c/Users/vihro/lera/products_list/src/components/Input/index.ts",[],"/mnt/c/Users/vihro/lera/products_list/src/components/Button/Button.tsx",[],"/mnt/c/Use